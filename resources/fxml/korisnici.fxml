<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<GridPane alignment="center" hgap="10" prefHeight="281.0" prefWidth="551.0" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <stylesheets>
      <URL value="@/css/style.css"/>
   </stylesheets>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <ListView fx:id="lwKorisnici" prefHeight="134.0" prefWidth="200.0" GridPane.rowSpan="5" />
      <Label text="Ime:" GridPane.columnIndex="1" />
      <Label text="Prezime:" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label text="E-mail:" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Label text="Korisničko ime:" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <Label text="Lozinka:" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <TextField fx:id="fldIme" GridPane.columnIndex="2" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="5">
        <buttons>
          <Button fx:id="btnDodaj" mnemonicParsing="false" text="Dodaj" onAction="#actionDodaj" />
            <Button fx:id="btnKraj" mnemonicParsing="false" text="Kraj" onAction="#actionKraj" />
        </buttons>
      </ButtonBar>
      <TextField fx:id="fldPrezime" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      <TextField fx:id="fldEmail" GridPane.columnIndex="2" GridPane.rowIndex="2" />
      <TextField fx:id="fldUsername" GridPane.columnIndex="2" GridPane.rowIndex="3" />
      <PasswordField fx:id="pswPassword" GridPane.columnIndex="2" GridPane.rowIndex="4" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
